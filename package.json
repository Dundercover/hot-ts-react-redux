{
  "name": "hot-ts-react-redux",
  "version": "1.0.0",
  "description": "Opinionated Hot React+Typescript+Redux with Parcel",
  "repository": {
    "type": "git",
    "url": "https://github.com/collingreen/hot-ts-react-redux-parcel"
  },
  "author": "Collin Green",
  "license": "MIT",
  "main": "src/index.tsx",
  "scripts": {
    "start": "parcel ./src/index.html --port 3000 --open",
    "build": "yarn run type-check && parcel build ./src/index.html --public-url '/'",
    "prettify": "cross-env prettier --write './src/**/*.{ts,tsx}'",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run prettify",
      "post-checkout": "yarn install"
    }
  },
  "engines": {
    "node": ">= 8.11.0",
    "npm": ">= 6.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/utils/set-up-tests.ts"
    ],
    "moduleNameMapper": {
      ".+\\.css$": "<rootDir>/src/utils/style-mock.ts"
    },
    "testMatch": [
      "**/*.(test|spec).(ts|tsx|js)"
    ]
  },
  "keywords": [
    "Typescript",
    "React",
    "Parcel"
  ],
  "dependencies": {
    "@testing-library/react": "^11.2.3",
    "@types/react-redux": "^7.1.15",
    "@types/webpack-env": "^1.16.0",
    "helpful-decorators": "^2.1.0",
    "husky": "^4.3.7",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hot-loader": "^4.13.0",
    "react-intl": "5.10.16",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-starter-kit": "^2.0.0",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^5.0.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.21",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.7",
    "babel-plugin-styled-components": "^1.12.0",
    "cross-env": "^7.0.3",
    "jest": "^26.6.3",
    "jest-dom": "^4.0.0",
    "jest-extended": "^0.11.5",
    "parcel-bundler": "^1.12.4",
    "react-test-renderer": "^17.0.1",
    "ts-jest": "^26.4.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "tslint-react": "^5.0.0",
    "typescript": "^4.1.3"
  }
}
